<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".ApplicationManager"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_icon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Capstone1"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name=".View.Splash"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".View.Main" />
        <activity android:name=".View.MyInfo" />
        <activity android:name=".View.LoginMain" />
        <activity android:name=".View.ChangePW" />
        <activity android:name=".View.FindIDAndPW" />
        <activity android:name=".View.Sign" />
        <activity android:name=".View.RestInfoView" />
        <activity android:name=".View.RegisterReview" />
        <activity android:name=".View.AllReview" />
    </application>

</manifest>